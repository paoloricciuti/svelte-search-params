		"tslib": "^2.8.0",
		"typescript": "^5.6.3",
		"typescript-eslint": "8.11.0",
		"vite": "^5.4.10",
		"vitest": "^2.1.3"
	},
	"type": "module",
	"peerDependencies": {
		"@sveltejs/kit": "^2.0.0",
		"svelte": "^5.0.0"
	},
	"exports": {
		"./package.json": "./package.json",
		".": "./package/index.js",
		"./sveltekit-search-params": "./package/sveltekit-search-params.svelte.js"
